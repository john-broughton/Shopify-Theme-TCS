          "settings": {
            "icon": "5-stars",
            "testimonial": "<p>\"Our registry experience was so thorough and fun! We cannot wait for our new dinnerwear!\"<\/p>",
            "image": "shopify:\/\/shop_images\/padgett.jpg",
